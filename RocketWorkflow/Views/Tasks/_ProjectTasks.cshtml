@model RocketWorkflow.Models.ViewModels.MainViewModel



<table class="table table-hover">
    <tbody>
    <tr class="table-primary">
        <th>
            @Html.DisplayNameFor(model => model.Task.TaskName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Task.AssignedEmployee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Task.DueDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Task.CompleteTime)
        </th>

    </tr>

        @foreach (var item in Model.TasksList)
        {
            <tr class="table-primary">
                <td>
                    @Html.DisplayFor(modelItem => item.TaskName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AssignedEmployee)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DueDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CompleteTime)
                </td>

                <td>
                    @Html.ActionLink("Edit", "Edit", new {id = item.TaskId}) |
                    @Html.ActionLink("Delete", "Delete", new {id = item.TaskId})
                </td>
            </tr>
        }
    </tbody>
</table>
<button class="btn btn-info">
    @Html.ActionLink("Create New", "Create", new { id = Model.Job.JobId})
</button>


